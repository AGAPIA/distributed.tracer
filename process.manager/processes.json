{
    "tracer.node": {
        "script": "../tracer.node/index.js",
        "cwd": "../tracer.node",
        "logfile": "../%s/logs/run-%s.log",
        "args": [
            "-c",
            "../config.json"
        ],
        "env": {
            "LD_LIBRARY_PATH": "./node_modules/node-river/serial.tracer/river.sdk/lin/lib/:/data/simpletracer/build-river-tools/lib"
        }
    },
    "state.aggregator": {
        "script": "../state.aggregator/index.js",
        "cwd": "../state.aggregator",
        "logfile": "../%s/logs/stateaggregator-%s.log",
        "args": [
            "-c",
            "../config.json"
        ]
    },
    "basic.fuzzer": {
        "script": "../../tracer.node/%s/fuzzer",
        "cwd": "../tracer.node/%s",
        "logfile": "../../%s/logs/basicfuzzer-%s.log",
        "autorestart": false,
        "args": [
            "../../%s/results",
            "-close_fd_mask=3",
            "-dump_to_db=1",
            "-config=../../config.json",
            "-print_final_stats=1",
            "-fast_corpus=0",
            "-results_csv=../../%s/logs/basic-%s.csv",
            "-binary_id=%s"
        ]
    },
    "eval.fuzzer": {
        "script": "../../tracer.node/%s/fuzzer",
        "cwd": "../tracer.node/%s",
        "logfile": "../../%s/logs/evalfuzzer-%s.log",
        "autorestart": false,
        "args": [
            "../../%s/results",
            "-close_fd_mask=3",
            "-initial_corpus_db=1",
            "-river=1",
            "-config=../../config.json",
            "-print_final_stats=1",
            "-fast_corpus=0",
            "-results_csv=../../%s/logs/eval-%s.csv",
            "-binary_id=%s"
        ]
    },
    "griver": {
        "script": "./genetic_code/main.py",
        "cwd": "../river.genetic",
        "logfile": "../%s/logs/river.genetic-%s.log",
        "interpreter": "/usr/bin/python3",
        "interpreterArgs": "-u",
        "args": [
            "--testsFolderCount",
            10,
            "--numTasks",
            1,
            "--isParallelExecution",
            0,
            "--populationSize",
            10,
            "--numberOfIterations",
            10,
            "--config",
            "../config.json",
            "--driver",
            1,
            "--isDebugEnabled",
            1,
            "--executableId",
            "%s"
        ],
        "env": {
            "SPARK_HOME": "/data/spark/spark-2.0.1-bin-hadoop2.7",
            "PYSPARK_PYTHON": "python3",
            "PYSPARK_DRIVER_PYTHON": "python3",
            "PYTHONPATH": "/data/spark/spark-2.0.1-bin-hadoop2.7/python:/data/spark/spark-2.0.1-bin-hadoop2.7/python/lib/py4j-0.10.3-src.zip",
            "JAVA_HOME": "/usr/lib/jvm/java-1.8.0-openjdk-amd64",
            "SPARK_WORKER_DIR": "/data/spark_worker_dir",
            "SPARK_EXECUTOR_INSTANCES": 2,
            "SCALA_HOME": "/usr/share/scala/lib",
            "SPARK_LOCAL_IP": "10.18.0.32",
            "PYTHONUNBUFFERED": 1,
            "SIMPLETRACERLOGSPATH": "%s/../%s/results",
            "SIMPLETRACERPATH":"/data/simpletracer/build-river-tools/bin/river.tracer",
            "LD_LIBRARY_PATH": "/data/simpletracer/build-river-tools/lib:/data/simpletracer/river.sdk/lin/lib"
        }
    }
}
